.positiona{
	width: 100%;
	position: absolute;
	top: 0;
	z-index: 100;
}
.navbar-inverse .navbar-inner{          /*清除导航背景默认样式*/
	background-color: transparent;
	background-image: none;
	border: none;
	box-shadow: none;
	filter:progid:DXImageTransform.Microsoft.gradient(startColorstr='#dd0170e3', endColorstr='#dd0170e3', GradientType=0);
}
.nav-bg{
	background-color: rgba(1,112,227,0.8);/*导航背景默认颜色－透明*/
	box-shadow: none;
}
.navbar-inverse .nav > li > a{          /*导航字体默认颜色*/
	box-shadow: none;
	color: #fff;
	padding: 18px 15px 18px 15px;
	line-height: 18px;
	/*padding-top: 0;
	padding-bottom: 0;
	height: 42px;
	line-height: 42px;*/
	text-shadow: none;
}
.navbar-inverse .nav > li > a:hover,  /*鼠标滑过导航时字体及背景颜色*/
.navbar-inverse .nav > li > a:active{  /*移动端点击导航时字体及背景颜色*/
	color: #fff;
	background-color: #0050a2;
}
.nav-bg .nav .active > a, 
.nav-bg .nav .active > a:hover, 
.nav-bg .nav .active > a:focus{          /*当前页面的菜单文字及背景颜色*/
	color: #fff;
	background-color: #0050a2;
	box-shadow: none;
}
.nav-bg .nav li.dropdown > .dropdown-toggle .caret{
	border-top-color:#fff;
	border-bottom-color: #fff;
}
.dropdown-style{          /*下拉菜单背景色及大小*/
	background-color: transparent;
	background-color: rgba(1,112,227,0.8);
	filter:progid:DXImageTransform.Microsoft.gradient(startColorstr='#dd0170e3', endColorstr='#dd0170e3', GradientType=0);
	border-radius: 0;
	border: none;
	margin: -1px 0 0;
	width: 160px;
	overflow: hidden;
}
.dropdown-style li a{      /*下拉菜单（二级）文字颜色*/
	color: #fff;
	padding: 8px 20px;
	height: 30px;
	line-height: 30px;
}
.nav .dropdown-style li a:hover{      /*鼠标滑过下拉菜单（二级）文字及背景颜色*/
	color: #fff;
	background-color: #0050a2;
	filter:progid:DXImageTransform.Microsoft.gradient(startColorstr='#0050a2', endColorstr='#0050a2', GradientType=0);
	background-image: none;
	text-decoration: none;
}
.navbar .nav > li > .dropdown-menu:after{      /*去除下拉菜单的小三角，ie不支持after*/
	display: none;
}
.nav-bg .nav li.dropdown.open > .dropdown-toggle, 
.nav-bg .nav li.dropdown.active > .dropdown-toggle, 
.nav-bg .nav li.dropdown.open.active > .dropdown-toggle,
.nav-bg .nav li.dropdown.open.active > .dropdown-toggle:focus,
.nav-bg .nav li.dropdown.open.active > .dropdown-toggle:active{    /*点击有下拉菜单的一级菜单的背景及文字颜色*/
	color: #fff;
	background-color: #0050a2;
}
