	 .nav-bg .nav-collapse .nav > li > a.dropdown-toggle:visited{    /*窄屏后一级、二级菜单背景颜色*/
	 	background-color: #0050a2; 
	 }
	 .navbar-inverse .nav-collapse .nav > li > a:hover,   /*覆盖bootstrap菜单收缩后点击一级访问后的颜色*/
	 .navbar-inverse .nav-collapse .nav > li > a:focus, 
